{
    "name": "petrgrishin/yiiapp-framework",
    "description": "Yiiapp framework",
    "license": "MIT",
    "require": {
        "php": ">=5.3.0",
        "yiisoft/yii": "dev-master",
        "esaverelatedbehavior/esaverelatedbehavior": "1.5"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "esaverelatedbehavior/esaverelatedbehavior",
                "version": "1.5",
                "dist": {
                    "type": "zip",
