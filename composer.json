{
    "name": "petrgrishin/yiiapp-framework",
    "description": "Yiiapp framework",
    "license": "MIT",
    "require": {
        "php": ">=5.3.0",
        "yiisoft/yii": "dev-master",
        "yiiext/with-related-behavior": "dev-master"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "yiiext/with-related-behavior",
                "source": {
                    "type": "git",
                    "url": "http://github.com/yiiext/with-related-behavior",
                    "reference": "master"
                },
                "autoload": {
                    "classmap" : ["."]
                }
            }
        }
    ],
    "autoload": {
        "psr-0": {
            "Yiiapp\\Framework": "src/"
        }
    }
}